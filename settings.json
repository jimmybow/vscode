{   // July 2021 (version 1.59)
    "explorer.confirmDelete": false,
    "multiCommand.commands": [
        {
            "command": "run_selected",
            "sequence": [
                "workbench.action.terminal.runSelectedText",
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "command": "run_selected_ipython",
            "sequence": [
                {"command": "workbench.action.terminal.sendSequence", "args": { "text": "%cpaste\u000D" }},
                "editor.action.clipboardCopyAction",
                "workbench.action.terminal.paste",
                "workbench.action.focusActiveEditorGroup",
                "cursorHome",
                {"command": "type", "args": { "text": "\u000D--\u000D" }},
                "cursorLeftSelect","cursorLeftSelect","cursorLeftSelect","cursorLeftSelect",
                "editor.action.clipboardCutAction",
                "workbench.action.terminal.paste",
                "workbench.action.focusActiveEditorGroup"
            ]
        }
    ],
    "update.mode": "none",
    "editor.tabCompletion": "on",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "explorer.confirmDragAndDrop": false,
    "python.terminal.activateEnvironment":false,
    "python.linting.pylintArgs": ["--disable=all", "--enable=F,E,unreachable,duplicate-key,unnecessary-semicolon,global-variable-not-assigned,unused-variable,binary-op-exception,bad-format-string,anomalous-backslash-in-string,bad-open-mode"]
}
